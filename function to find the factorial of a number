import java.util.*;

class Main{
    public static int factorial(int n){
        int product = 1;
        while(n != 0){
            product = product * n;
            n--;
        }
        return product;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println(factorial(n));
    }
}
