import java.util.*;

class Main{
    public static int largestOfThree(int n1, int n2, int n3) {
        int large = 0;
        large = (n1 > n2 && n1 > n3) ? n1 : (n2 > n3 ? n2 : n3);
        return large;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n1 = sc.nextInt();
        int n2 = sc.nextInt();
        int n3 = sc.nextInt();
        
        System.out.println(largestOfThree(n1, n2, n3));
    }
}

input: 10, 5, 7
outptu: 10
