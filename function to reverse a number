import java.util.*;

class Main{
    public static int reverseTheNumber(int n){
        int rev = 0;
        while(n != 0){
            rev = rev * 10 + (n % 10);
            n /= 10;
        }
        return rev;
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n1 = sc.nextInt();
        System.out.println(reverseTheNumber(n1));
    }
}

simple input : 12345
output : 54321
